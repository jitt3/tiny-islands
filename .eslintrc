{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended","eslint-config-prettier","plugin:react/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "strict": ["error", "never"]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
